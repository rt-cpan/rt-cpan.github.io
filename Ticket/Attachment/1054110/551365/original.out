<script language="JavaScript" type="text/JavaScript">
	function validate(myform)
	{
		if(myform.loginName.value=="")
        {
            alert('Please enter a login name in the form of an email address');
            return false;
        }
        if(myform.password.value=="")
        {
            alert('Please enter your password');
            return false;
        }
        var ln = myform.loginName.value;
        if(!ln.match('^([0-9a-zA-Z]+[-._\'+&amp;])*[0-9a-zA-Z]+@([-_0-9a-zA-Z]+[.])+[a-zA-Z]{2,6}$'))
        {
        	alert ('The Login ID must be a valid email address');
        	return false;
        }
        setOffset(myform);
	}
	
	function setOffset(myform){
		var mydate= new Date();
		var differenceInMinutes = mydate.getTimezoneOffset();
		myform.timeOffSet.value=differenceInMinutes;
	}
</script>
