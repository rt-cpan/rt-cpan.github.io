<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions targetNamespace="http://www.example.com/webservice"
    xmlns:http="http://schemas.xmlsoap.org/wsdl/http/"
    xmlns:mime="http://schemas.xmlsoap.org/wsdl/mime/"
    xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
    xmlns:wsoap12="http://schemas.xmlsoap.org/wsdl/soap12/"
    xmlns:soapenc="http://schemas.xmlsoap.org/soap/encoding/"
    xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ws="http://www.example.com/webservice">
    <wsdl:types>
        <xs:schema targetNamespace="http://www.example.com/otherSchema">
            <xs:element name="testDetails"/>
        </xs:schema>
    </wsdl:types>
    <wsdl:message name="testInReq">
        <wsdl:part name="testInReq" element="ns:testDetails" xmlns:ns="http://www.example.com/otherSchema"/>
    </wsdl:message>
    <wsdl:portType name="testPort">
        <wsdl:operation name="testOp">
            <wsdl:input name="testIn" message="ws:testInReq"/>
         </wsdl:operation>
    </wsdl:portType>
    <wsdl:binding name="testBinding" type="ws:testPort">
        <soap:binding transport="http://schemas.xmlsoap.org/soap/http" style="document"/>
        <wsdl:operation name="testOp">
            <soap:operation soapAction="testOp" style="document"/>
            <wsdl:input name="testIn">
                <soap:body use="literal"/>
             
            </wsdl:input>
        </wsdl:operation>
    </wsdl:binding>
    
</wsdl:definitions>
