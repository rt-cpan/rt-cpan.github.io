#!/usr/bin/perl

use strict;
use warnings;
use PPI::Document;

my $source = 'my @foo = (1 .. 10);';
my $ra_ops;
{
	my $Document = PPI::Document->new(\$source);
	$ra_ops = $Document->find('PPI::Token::Operator');
}
test($ra_ops);


sub test {
	my $raops = shift;

	foreach my $op (@{$ra_ops}) {
		if ($op->content eq '..') {
			print "Ok\n" ;
			exit;
		}
	}

	print "NOT Ok\n";
}
