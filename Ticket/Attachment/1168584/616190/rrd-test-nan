-			'bytesOut min nan', 'bytesOut max nan', 'bytesOut last nan'
+			'bytesIn min -nan', 'bytesIn max -nan', 'bytesIn last -nan',
+			'bytesOut min -nan', 'bytesOut max -nan', 'bytesOut last -nan'
 		)],
 	'annual' => [(
-			'bytesIn min nan', 'bytesIn max nan', 'bytesIn last nan',
-			'bytesOut min nan', 'bytesOut max nan', 'bytesOut last nan'
+			'bytesIn min -nan', 'bytesIn max -nan', 'bytesIn last -nan',
+			'bytesOut min -nan', 'bytesOut max -nan', 'bytesOut last -nan'
 		)],
