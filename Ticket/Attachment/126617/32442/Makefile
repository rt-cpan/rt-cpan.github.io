	$(RM_F) blib\lib\SQL\Translator\Parser\SQLite.pm $(MAKEFILE_OLD) blib\lib\SQL\Translator.pm blib\lib\SQL\Translator\Parser.pm blib\lib\SQL\Translator\Parser\DBI\MySQL.pm blib\lib\SQL\Translator\Parser\YAML.pm
