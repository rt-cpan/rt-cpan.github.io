	$(RM_F) blib\lib\SQL\Translator\Producer\SQLite.pm $(FIRST_MAKEFILE) blib\lib\SQL\Translator\Parser\Sybase.pm blib\lib\SQL\Translator\Producer\MySQL.pm blib\lib\SQL\Translator\Parser\XML.pm
	$(RM_F) blib\lib\SQL\Translator\Parser\Excel.pm blib\lib\SQL\Translator\Producer\Sybase.pm blib\lib\SQL\Translator\Producer.pm blib\lib\SQL\Translator\Schema\Constants.pm
