	$(NOECHO) $(NOOP)

realclean ::
	$(RM_F) \
	  $(INST_SCRIPT)\podview.pl $(INST_SCRIPT)\fmview.pl \
	  $(INST_SCRIPT)\tmlink.pl $(INST_SCRIPT)\VB.pl \
	  $(INST_SCRIPT)\p-class.pl $(INST_SCRIPT)\gencls.pl \
	  $(INST_SCRIPT)\cfgmaint.pl 

