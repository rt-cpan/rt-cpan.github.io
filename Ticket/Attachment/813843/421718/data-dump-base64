#!/usr/local/bin/perl -w

use strict;
use utf8;

use Devel::Peek;
use Data::Dump qw(pp);

my $s = "\x{c4}\x{cf}\x{cd}\x{c5}\x{ce} ROLANDBERGER.RU: \x{d2}\x{c5}\x{c7}\x{c9}\x{d3}\x{d4}\x{d2}\x{c1}\x{c3}\x{c9}\x{d1} \x{d0}\x{d2}\x{cf}\x{c4}\x{cc}\x{c5}\x{ce}\x{c1} \x{c4}\x{cf} 01-MAY-2004";
utf8::upgrade($s);

print STDERR '$s before pp(): '; Dump($s);
print STDERR "\npp(\$s): " . pp($s) . "\n\n";
print STDERR '$s after pp(): '; Dump($s);
