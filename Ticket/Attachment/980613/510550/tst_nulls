
use Spreadsheet::ParseExcel::Stream;

my $xls = Spreadsheet::ParseExcel::Stream->new("t/test_nulls.xls");

while ( my $sheet = $xls->sheet() ) {
  print "New sheet\n";
  while ( my $row = $sheet->row() ) {
    no warnings 'uninitialized';
    print "[$_]" for @$row;
    print "\n";
  }
}
close $fh;
